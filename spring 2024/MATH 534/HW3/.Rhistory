pow <- function(teta, teta_0, n, sig = 1, zstar) {
right_bound <- pnorm(q = (teta_0 - teta) * sqrt(n) / sig + zstar)
left_bound <- pnorm(q = (teta_0 - teta) * sqrt(n) / sig - zstar)
p <- 1 - right_bound + left_bound
return(p)
}
